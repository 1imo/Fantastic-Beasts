RewriteEngine On

# Redirect /index.php to the canonical homepage URL
RewriteCond %{THE_REQUEST} \s/+index\.php[\s?] [NC]
RewriteRule ^index\.php$ / [R=301,L]

# Force non-www as canonical (https://fantasticbeasts.world)
RewriteCond %{HTTP_HOST} ^www\.fantasticbeasts\.world$ [NC]
RewriteRule ^(.*)$ https://fantasticbeasts.world/$1 [R=301,L]

